{
  "name": "@org/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --parallel --target=serve --projects=shortener,shortener-api",
    "start": "nx reset && nx build shortener --prod --emptyOutDir && nx serve shortener-api --prod",
    "sonar": "npm run sonar-frontend && npm run sonar-backend",
    "sonar-frontend": "cd ./apps/shortener && node sonar-project.js",
    "sonar-backend": "cd ./apps/shortener-api && node sonar-project.js",
    "ts-coverage-frontend": "npx typescript-coverage-report --project ./apps/shortener/tsconfig.app.json",
    "ts-coverage-backend": "npx typescript-coverage-report --project ./apps/shortener-api/tsconfig.app.json -o ./dist/apps/coverage-ts-backend",
    "test-e2e-frontend": "nx e2e shortener-e2e",
    "test-e2e-backend": "npm run seed && nx e2e shortener-api-e2e",
    "test-unit-backend": "nx test shortener-api",
    "test-smoke-backend": "npx artillery run --environment load ./scenarios/scenario.yml",
    "up": "docker compose up --build -d",
    "down": "docker compose down",
    "seed": "node -e 'require(\"./seeding/seeder.js\").seed()'"
  },
  "private": true,
